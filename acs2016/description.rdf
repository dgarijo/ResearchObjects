    <dc:title>Research Object for the thesis "Mining Abstractions in Scientific Workflows"</dc:title>
    <schema:contributor rdf:nodeID="_88a814cb-e1f6-4a66-bdae-82c168a093f8"/>
    <schema:contributor rdf:nodeID="_1c3694b3-66f6-4b98-912d-a40e6303b627"/>
